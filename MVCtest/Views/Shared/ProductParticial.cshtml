@model MVCtest.Models.Product
@{
    ViewBag.Title = "ProductParticial";
}

<h2>ProductParticial</h2>



<div class="col-xl-3 col-lg-4 col-md-6 col-sm-12">
    <div class="card" style="width: 18rem;">
        <img src="~/Assets/images/" class="card-img-top" >
        <div class="card-body">
            <p class="card-text text-center">@Model.Product_Name</p>
            <p class="card-text text-center">@Model.UnitPrice</p>
        </div>
    </div>
</div>